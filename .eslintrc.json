{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "plugins": ["simple-import-sort"],
  "rules": {
    "simple-import-sort/imports": [
      "error",
      {
        "groups": [["^react"], ["^@?\\w"], ["^@/?\\w"], ["^\\./"], ["^\\.\\."]]
      }
    ],
    "simple-import-sort/exports": "error",
    "@typescript-eslint/no-empty-object-type": "off"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}
